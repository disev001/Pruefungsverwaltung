<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane maxHeight="520.0" maxWidth="560.0" minHeight="520.0" minWidth="560.0" prefHeight="520.0" prefWidth="560.0"
            xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="de.fh.swf.inf.se.view.NotenListeController">
    <children>
        <SplitPane dividerPositions="0.5" orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0">
            <items>
                <AnchorPane>
                    <children>
                        <TableView id="notenTable" fx:id="notenTable" editable="true" prefHeight="367.0"
                                   prefWidth="564.0">
                            <columns>
                                <TableColumn id="tc_fach" fx:id="tc_fach" prefWidth="303.0" text="Fach"/>
                                <TableColumn id="tc_note" fx:id="tc_note" minWidth="0.0" prefWidth="72.0" text="Note"/>
                                <TableColumn id="tc_cp" fx:id="tc_cp" prefWidth="67.0" text="CP"/>
                                <TableColumn id="tc_versuch" fx:id="tc_versuch" prefWidth="114.0" text="Versuch"/>
                            </columns>
                        </TableView>
                    </children>
                </AnchorPane>
                <AnchorPane>
                    <children>
                        <GridPane prefHeight="173.0" prefWidth="557.0">
                            <columnConstraints>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="274.0" minWidth="10.0"
                                                   prefWidth="155.0"/>
                                <ColumnConstraints hgrow="SOMETIMES" maxWidth="490.0" minWidth="10.0"
                                                   prefWidth="402.0"/>
                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints maxHeight="53.0" minHeight="10.0" prefHeight="32.0" vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="84.0" minHeight="10.0" prefHeight="35.0" vgrow="SOMETIMES"/>
                                <RowConstraints maxHeight="92.0" minHeight="10.0" prefHeight="92.0" vgrow="SOMETIMES"/>
                            </rowConstraints>
                            <children>
                                <Button id="btn_save" fx:id="btn_save" mnemonicParsing="false" text="Save"
                                        GridPane.rowIndex="2">
                                    <GridPane.margin>
                                        <Insets bottom="20.0" left="20.0"/>
                                    </GridPane.margin>
                                </Button>
                                <Button id="btn_extra" fx:id="btn_extra" mnemonicParsing="false" text="Abschlussnoten"
                                        GridPane.columnIndex="1" GridPane.rowIndex="2">
                                    <GridPane.margin>
                                        <Insets bottom="20.0" left="20.0"/>
                                    </GridPane.margin>
                                </Button>
                                <Label text="Notendurchschnitt:">
                                    <GridPane.margin>
                                        <Insets left="20.0"/>
                                    </GridPane.margin>
                                </Label>
                                <Label text="Gesammt CP:" GridPane.rowIndex="1">
                                    <GridPane.margin>
                                        <Insets left="20.0"/>
                                    </GridPane.margin>
                                </Label>
                                <Label id="lbl_note" fx:id="lbl_note" text="Label" GridPane.columnIndex="1">
                                    <GridPane.margin>
                                        <Insets left="20.0"/>
                                    </GridPane.margin>
                                </Label>
                                <Label id="lbl_cp" fx:id="lbl_cp" text="Label" GridPane.columnIndex="1"
                                       GridPane.rowIndex="1">
                                    <GridPane.margin>
                                        <Insets left="20.0"/>
                                    </GridPane.margin>
                                </Label>
                            </children>
                        </GridPane>
                    </children>
                </AnchorPane>
            </items>
        </SplitPane>
    </children>
</AnchorPane>